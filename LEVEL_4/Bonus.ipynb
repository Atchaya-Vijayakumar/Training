    "import hashlib\n",
    "sample_string = \"hello\"\n",
    "encoded_string = sample_string.encode()\n",
    "hash_object = hashlib.sha256(encoded_string)\n",
    "hex_hash = hash_object.hexdigest()\n",
    "print(\"SHA256 hash:\", hex_hash)\n"
